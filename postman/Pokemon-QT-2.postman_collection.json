				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Tipo retornado é 'water'\", () => {\r",
							"    pm.expect(pm.response.json().name).to.eql(\"water\");\r",
							"});\r",
							"\r",
							"//Teste para verificar se os campos existem na resposta da API \r",
							"pm.test(\"Estrutura básica do tipo está presente\", () => { \r",
							"    const body = pm.response.json(); \r",
							"    pm.expect(body).to.have.property(\"id\"); \r",
							"    pm.expect(body).to.have.property(\"name\"); \r",
							"    pm.expect(body).to.have.property(\"damage_relations\"); \r",
							"    pm.expect(body).to.have.property(\"pokemon\"); \r",
							"    pm.expect(body).to.have.property(\"moves\"); \r",
							"});\r",
							"\r",
							"//Teste para verificar os tipos de pokemons que causam dano em pokemon tipo agua -> eletrico e grama \r",
							"pm.test(\"Water tem fraquezas corretas\", () => { \r",
							"    const damage = pm.response.json().damage_relations; \r",
							"    const fraquezas = damage.double_damage_from.map(t => t.name); \r",
							"    pm.expect(fraquezas).to.include(\"electric\"); \r",
							"    pm.expect(fraquezas).to.include(\"grass\"); \r",
							"});\r",
							"\r",
							"//Teste para verificar os tipos de pokemons que agua se sobresai -> fogo, terra e pedra \r",
							"pm.test(\"Water é super efetivo contra fire/ground/rock\", () => { \r",
							"    const damage = pm.response.json().damage_relations; const efetivo = damage.double_damage_to.map(t => t.name); pm.expect(efetivo).to.include(\"fire\"); \r",
							"    pm.expect(efetivo).to.include(\"ground\"); \r",
							"    pm.expect(efetivo).to.include(\"rock\"); \r",
							"});\r",
							"\r",
							"//Teste para verificar se 'squirtle' e 'psyduck' fazem parte da lista de pokemons retornados \r",
							"pm.test(\"Pokémon famosos de água estão listados\", () => { \r",
							"    const nomes = pm.response.json().pokemon.map(p => p.pokemon.name); \r",
							"    pm.expect(nomes).to.include(\"squirtle\"); \r",
							"    pm.expect(nomes).to.include(\"psyduck\"); \r",
							"});\r",
							"\r",
							"//Teste para verificar se há golpes do tipo water cadastrados \r",
							"pm.test(\"Water possui golpes associados\", () => { \r",
							"    const moves = pm.response.json().moves; \r",
							"    pm.expect(moves.length).to.be.above(0); \r",
							"});"
						],
